<h1>Ratings</h1>
<table class="table table-hover">
	<tr>
		<th>Organization Name</th>
		<th>Organization Email</th>
		<th>Fits Svp's Criteria</th>
		<th>Potential SVP Impact</th>
		<th>Level of Excitement</th>
		<th>Benefit to SVP</th>
		<th>Invite to Submit RFP?</th>
	</tr>
	<% @ratings.each do |rating| %>
		<tr>
			<td><%= rating.loi.name %></td>
			<td><%= rating.loi.email %></td>
			<td><%= rating.q1 %></td>
			<td><%= rating.q2 %></td>
			<td><%= rating.q3 %></td>
			<td><%= rating.q4 %></td>
			<td><%= rating.q5 %></td>
		</tr>
	<% end %>
</table>
<h1>Ratings</h1>
<% @ratings.each do |rating| %>
	<%= rating.user_id %>
	<%= rating.loi_id %>
	<%= rating.rating_fields%>
<% end %>
